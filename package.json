{
  "name": "victoury-public-api-mcpserver",
  "version": "0.1.0",
  "description": "MCP server for Victoury Public API v2 integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "http-server": "node dist/http-server-simple.js",
    "mcp-stdio": "node dist/mcp-stdio-server.js",
    "mcp-sse": "node dist/mcp-sse-server.js",
    "dev:http": "tsx watch src/http-server.ts",
    "dev:mcp-stdio": "tsx watch src/mcp-stdio-server.ts",
    "dev:mcp-sse": "tsx watch src/mcp-sse-server.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "mcp",
    "victoury",
    "api",
    "travel"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "zod": "^3.25.42"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.27",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
